{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.autoSave": "onFocusChange",
  "graphql.trace.server": "messages",
  "git.alwaysSignOff": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "debug.toolBarLocation": "docked",
  "git.postCommitCommand": "push",
  "sqltools.autoOpenSessionFiles": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "prettier.requireConfig": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "java.semanticHighlighting.enabled": true,
  "java.codeGeneration.useBlocks": true,
  "java.import.gradle.arguments": "",
  "java.import.gradle.home": "",
  "java.import.gradle.jvmArguments": "",
  "java.import.gradle.user.home": "",
  "java.import.gradle.version": "",
  "java.saveActions.organizeImports": true,
  "sqltools.useNodeRuntime": true,
  "extensions.ignoreRecommendations": false,
  "gitSemanticCommit.preserveScope": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.debugCodeLens.showWhenTestStateIn": [
    "pass",
    "fail",
    "unknown"
  ],
  "jest.runAllTestsFirst": false,
  "jest.autoEnable": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "pgFormatter.noGrouping": true,
  "pgFormatter.wrapLimit": 100,
  "sql-formatter.uppercase": true,
  "sqltools.format": {
    "linesBetweenQueries": 2,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ]
}
