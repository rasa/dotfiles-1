;;;
; this file is generated edit ~/.config/yadm/alt/.gitconfig##template instead
;;;
[user]
  name = Caleb Cushing
  email = caleb@7pi.co
[push]
  default = current
[core]
  autocrlf = input
[alias]
  csa = commit --signoff --all
  fap = fetch --all --prune --prune-tags
  pushf = push --force-with-lease
  mush = merge --squash
  blow = !git branch -d $1 && !git push origin :$1 && :
[rerere]
  enabled = true
[checkout]
  defaultRemote = origin
[pager]
  status = true
[merge]
  tool = meld
  guitool = meld
[commit]
  template = ~/.gitmessaage
